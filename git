pipeline {
    agent any

    stages {
        stage('Clone from GitHub') {
            steps {
                git url: 'https://github.com/Anusha-90/Git-Jenkins-Docker-K8s/new/main', branch: 'main'
            }
        }

        stage('Run Script') {
            steps {
                sh 'echo "Hello from Jenkins!"'
            }
        }
    }
}
